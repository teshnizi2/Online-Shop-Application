<AnchorPane prefHeight="800.0" prefWidth="1550.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphics.AdminProfile.Categories.AddCategory">
    <children>
        <Label layoutX="468.0" layoutY="156.0" text="Category Name">
            <font>
                <Font name="Times New Roman" size="24.0" />
            </font>
        </Label>
        <Label layoutX="465.0" layoutY="219.0" text="Special Features">
            <font>
                <Font name="Times New Roman" size="24.0" />
            </font>
        </Label>
        <TextField fx:id="categoryNameField" layoutX="672.0" layoutY="148.0">
            <font>
                <Font name="Times New Roman" size="24.0" />
            </font>
        </TextField>
        <TextField fx:id="specialFeaturesField" layoutX="672.0" layoutY="211.0">
            <font>
                <Font name="Times New Roman" size="24.0" />
            </font>
        </TextField>
        <ListView fx:id="specialFeaturesList" layoutX="672.0" layoutY="330.0" prefHeight="258.0" prefWidth="300.0" />
        <Button layoutX="850.0" layoutY="255.0" mnemonicParsing="false" onMouseClicked="#addFeature" prefHeight="44.0" prefWidth="122.0" text="Add Feature">
            <font>
                <Font name="Times New Roman" size="18.0" />
            </font>
        </Button>
