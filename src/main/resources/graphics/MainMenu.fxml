<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="1550.0" style="-fx-background-color: #F7F7F7;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphics.MainMenu">
   <children>
      <Button layoutX="201.0" layoutY="387.0" mnemonicParsing="false" onAction="#exit" prefHeight="44.0" prefWidth="177.0" style="-fx-background-radius: 100;" text="Exit">
         <font>
            <Font name="Times New Roman" size="36.0" />
         </font>
      </Button>
      <ImageView fx:id="accountImage" fitHeight="44.0" fitWidth="45.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/account.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="800.0" fitWidth="1021.0" layoutX="529.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/store.png" />
         </image>
      </ImageView>
      <Button fx:id="profileButton" layoutX="48.0" layoutY="274.0" mnemonicParsing="false" onAction="#goToProfileMenu" prefHeight="54.0" prefWidth="484.0" style="-fx-background-radius: 100;" text="Profile">
         <font>
            <Font name="Times New Roman" size="36.0" />
         </font>
      </Button>
      <Button fx:id="productsMenuButton" layoutX="48.0" layoutY="172.0" mnemonicParsing="false" onAction="#goToProductsMenu" prefHeight="69.0" prefWidth="484.0" style="-fx-background-radius: 100;" text="Products Menu">
         <font>
            <Font name="Times New Roman" size="36.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
